               <b><dtml-var "itm['name']"></b>
               <dtml-if "itm.get('performer',[])">
                <div style="color: blue; padding-left:2px; padding-right:2px; padding-top:0px; padding-bottom:0px; margin-left: 4px; margin-right: 4px;">
                 Performer
                </div>
