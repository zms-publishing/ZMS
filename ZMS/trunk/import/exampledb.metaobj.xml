<item type="dictionary"><dictionary>
<item key="dtml"><![CDATA[<!-- BO bodyContentZMSCustom_exampleDatabase -->

<dtml-call "REQUEST.set('templateUrl','%s/bodyContentZMSCustom_%s'%(absolute_url(),meta_id))">

<dtml-let nav="['dept','Departments','emp','Employees']" nav_key="'NAV_%s'%meta_id.upper()">

<dtml-if "REQUEST.form.has_key(nav_key)">
 <dtml-call "SESSION.set(nav_key,REQUEST.form[nav_key])">
</dtml-if>
<dtml-call "SESSION.set(nav_key,SESSION.get(nav_key,nav[0*2]))">

<br/>
<dtml-let i="nav.index(SESSION[nav_key])" key="nav[i]" value="nav[i+1]">
 <table cellspacing="0" cellpadding="0" border="0" width="480">
 <tr>
  <td nowrap="nowrap" width="120" align="center" style="border-left:1px solid gray; border-top:1px solid gray; border-right:1px solid black;" class="primColorLight0">
   <a href="<dtml-var "url_append_params(getHref2IndexHtml(REQUEST),{nav_key:key})">" class="navMicro"><b>&nbsp;<dtml-var value>&nbsp;</b></a>
  </td>
  <td width="1" style="border-bottom:1px solid gray; background-color:#AAAAAA"><img src="<dtml-var spacer_gif>" title="" border="0" width="1"></td>
  <td width="1" style="border-bottom:1px solid gray; background-color:#CCCCCC"><img src="<dtml-var spacer_gif>" title="" border="0" width="1"></td>
  <td width="1" style="border-bottom:1px solid gray; background-color:#EEEEEE"><img src="<dtml-var spacer_gif>" title="" border="0" width="1"></td>
  <td nowrap="nowrap" width="356" align="right" style="border-bottom:1px solid gray;">
   <dtml-in "_.range(0,_.len(nav)/2)">
    <dtml-let j="_['sequence-item']" key="nav[j*2]" value="nav[j*2+1]">
     <dtml-if "i!=j*2">
      |&nbsp;<a href="<dtml-var "url_append_params(getHref2IndexHtml(REQUEST),{nav_key:key})">" class="navMicro"><dtml-var value></a>&nbsp;
     </dtml-if>
    </dtml-let>
   </dtml-in>
  </td>
 </tr>
 <tr>
  <td colspan="5" align="left" valign="top" style="border-left:1px solid gray; border-right:1px solid black; border-bottom:1px solid black;" class="primColorLight0">
   <table cellspacing="0" cellpadding="6" border="0" width="100%">
   <tr>
    <td align="left" valign="top">
     <dtml-with "_[SESSION.get(nav_key)]">
     <dtml-let metaObj="getMetaobj(meta_id)">

      <!--// BO Filter records //-->
      <dtml-let fmName="'filterForm'">
      <dtml-if "REQUEST.get('btn','')==getLangStr('BTN_SEARCH',lang)+'...'">
       <dtml-call "SESSION.set('qfilter',REQUEST.get('qfilter',''))">
      <dtml-elif "REQUEST.get('btn','')==getLangStr('BTN_RESET',lang)">
       <dtml-call "SESSION.set('qfilter','')">
      </dtml-if>
      <form name="<dtml-var fmName>" action="<dtml-var URL>" method="get">
      <table>
      <tr valign="middle">
       <td nowrap="nowrap">
        <b><dtml-var "getLangStr('ATTR_FILTER',lang)"></b>
       </td>
       <td nowrap="nowrap">
        <dtml-var "getTextInput(fmName=fmName,elName='qfilter',size=20,value=SESSION.get('qfilter',''))">
       </td>
       <td nowrap="nowrap">
        <input type="submit" name="btn" value="<dtml-var "getLangStr('BTN_SEARCH',lang)">...">
        <input type="submit" name="btn" value="<dtml-var "getLangStr('BTN_RESET',lang)">">
       </td>
      </tr>
      </table>
      </form>
      </dtml-let>
      <!--// EO Filter records //-->

      <!--// BO Init parameters //-->
      <dtml-call "REQUEST.set('qstart',REQUEST.get('qstart',0))">
      <dtml-call "REQUEST.set('qbatch',REQUEST.get('qbatch',5))">
      <dtml-call "REQUEST.set('qsize',REQUEST.get('qsize',10))">
      <dtml-call "recordSet_Init(REQUEST)">
      <dtml-call "REQUEST.set('res',copy_list(res))">
      <dtml-call "REQUEST.set('res',filter_list(res,_.None,SESSION.get('qfilter','')))">
      <dtml-call "recordSet_Sort(REQUEST)">
      <!--// EO Init parameters //-->

      <!--// BO Display record //-->
      <dtml-if "REQUEST.get('action','')=='choose'">
       <dtml-var "metaobj_record_select(_,_,metaObj=metaObj,row=res[qindex])"><br/><br/>
      </dtml-if>
      <!--// EO Display record //-->

      <!--// BO Display grid //-->
      <dtml-if "REQUEST.get('action','')==''">
       <dtml-var "metaobj_recordset_main_grid(_,_,grid_options=['choose'])">
      </dtml-if>
      <!--// EO Display grid //-->

     </dtml-let>
     </dtml-with>
    </td>
   </tr>
   </table>
  </td>
 </tr>
 </table>
</dtml-let>
<br/>

</dtml-let>

<!-- EO bodyContentZMSCustom_exampleDatabase -->]]></item>
