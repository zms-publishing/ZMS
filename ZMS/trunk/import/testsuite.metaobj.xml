<item type="dictionary"><dictionary>
<item key="custom"><![CDATA[<!-- BO bodyContentZMSCustom_ZMSPyUnitTest -->

<div class="form-label"><dtml-var "getObjProperty('title',REQUEST)"></div>
<table border="1">
 <dtml-in "getMetaobjAttrIds(meta_id)"
  ><dtml-let key=sequence-item metaObjAttr="getMetaobjAttr(meta_id,key)"
   ><dtml-if "metaObjAttr['type']=='text'"
    ><tr valign="top"
     ><td><div class="form-small"><b><dtml-var "metaObjAttr['name']"></b></div
     ><td><div class="form-small"
      ><code
       ><dtml-let l="string_list(getObjAttrValue(getObjAttr(key),REQUEST))"
        ><dtml-in "_.range(_.len(l)/2)"
         ><dtml-let i=sequence-item label="l[i*2]" dtml_body="l[i*2+1]" dtml="[]"
          ><div class="form-small"
           ><dtml-var label
           ><dtml-if "REQUEST.get('action')=='run'"
            ><dtml-call "dtml.append('<dtml-try>')"
            ><dtml-call "dtml.append('<dtml-let context='+QUOT+'getParentNode()'+QUOT+'>')"
            ><dtml-call "dtml.append(dtml_body)"
            ><dtml-call "dtml.append('<span style=\'color:green;\'>Success!</span>')"
            ><dtml-call "dtml.append('</dtml-let>')"
            ><dtml-call "dtml.append('<dtml-except>')"
            ><dtml-call "dtml.append('<span style=\'color:red;\'>Failed!</span>')"
            ><dtml-call "dtml.append('<dtml-var error_value>: <dtml-var error_type><br/>')"
            ><dtml-call "dtml.append('</dtml-try>')"
            ><dtml-var "dt_html(''.join(dtml),REQUEST)"
           ></dtml-if
          ></div
         ></dtml-let
        ></dtml-in
       ></dtml-let
      ></code></div></td
    ></tr
   ></dtml-if
  ></dtml-let
 ></dtml-in>
</table>

<!-- EO bodyContentZMSCustom_ZMSPyUnitTest -->
]]></item>
