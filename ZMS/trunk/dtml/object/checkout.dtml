<dtml-if "isCheckedOut(REQUEST) and 
          _.hasattr(this(),'co_uid_%s'%lang) and
          _.getattr(this(),'co_uid_%s'%lang,_.None) != _.str(AUTHENTICATED_USER) and
          not (REQUEST.has_key('btn') and REQUEST['btn'] == getZMILangStr('BTN_OVERTAKE'))">
  <dtml-call "RESPONSE.redirect('manage_checkout?lang=%s'%lang)">
  <dtml-return "_.False">
<dtml-else>
  <dtml-call "this().checkOut(REQUEST)">
  <dtml-return "_.True">
</dtml-if>
