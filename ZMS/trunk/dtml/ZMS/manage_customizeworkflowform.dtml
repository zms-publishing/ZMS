                <dtml-in "itm.get('performer',[])">
                 <dtml-let role=sequence-item role_str="'ROLE_%s'%role.upper()" role_lang_str="getZMILangStr(role_str)">
                  <dtml-if "role_str==role_lang_str">
                   <dtml-var role><br/>
                  <dtml-else>
                   <dtml-var role_lang_str><br/>
                  </dtml-if>
                 </dtml-let>
                </dtml-in>
