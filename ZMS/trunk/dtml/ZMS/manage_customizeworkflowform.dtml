    <tr valign="top">
     <td><dtml-var "getSelect(fmName='trForm', elName='newFrom:list', value='', inputtype='multiline', lang_str='From', required=0, optpl=wfItemOptpl, enabled=1, REQUEST=REQUEST, css='form-small')"></td>
