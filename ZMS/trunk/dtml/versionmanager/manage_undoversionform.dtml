             v.<dtml-var "getObjProperty('master_version',REQUEST)">.<dtml-var "getObjProperty('major_version',REQUEST)">.<dtml-var "getObjProperty('minor_version',REQUEST)">
             <dtml-if "change_dt is not _.None and change_dt > last_version_dt"><dtml-var "getObjProperty('change_uid',REQUEST)"> <dtml-var "getLangFmtDate(change_dt)"></dtml-if>
             <br/>
            </dtml-if>
