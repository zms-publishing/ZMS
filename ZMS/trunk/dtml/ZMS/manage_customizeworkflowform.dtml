   */
  function onWorkflowFormSubmit(fm) 
  {
    var b = true;
    var el = fm.elements["workflow:int"];
    <dtml-unless "getAutocommit()==1">
     if (!el.checked) {
      if (self.btnClicked=="<dtml-var "getZMILangStr('BTN_CHANGE')">") {
